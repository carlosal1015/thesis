@ARTICLE{Triantafyllou2005-zk,
  title       = "Comparison of physiological noise at 1.5 T, 3 {T} and 7 {T}
                 and optimization of {fMRI} acquisition parameters",
  author      = "Triantafyllou, C and Hoge, R D and Krueger, G and Wiggins, C J
