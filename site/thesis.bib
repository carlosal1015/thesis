@ARTICLE{Glatard2015-fn,
  title     = "Reproducibility of neuroimaging analyses across operating
               systems",
  author    = "Glatard, Tristan and Lewis, Lindsay B and Ferreira da Silva,
               Rafael and Adalat, Reza and Beck, Natacha and Lepage, Claude and
