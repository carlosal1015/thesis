@ARTICLE{Bissett2013-ep,
  title       = "Stop before you leap: changing eye and hand movements requires
                 stopping",
  author      = "Bissett, Patrick G and Logan, Gordon D",
  affiliation = "Department of Psychology, Vanderbilt University, Nashville, TN
