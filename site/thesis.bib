@ARTICLE{Sporns2012-fc,
  title       = "From simple graphs to the connectome: networks in neuroimaging",
  author      = "Sporns, Olaf",
  affiliation = "Department of Psychological and Brain Sciences, Indiana
                 University, Bloomington, IN 47405, USA. osporns@indiana.edu",
  abstract    = "Connectivity is fundamental for understanding the nature of
                 brain function. The intricate web of synaptic connections
